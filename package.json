{
  "name": "sfasb_management",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds run"
  },
  "private": true,
  "cds": {
    "build": {
      "target": "gen"
    },
    "requires": {
      "db": {
        "kind": "hana-cloud"
      },
      "connectivity": true,
      "destinations": true,
      "html5-repo": true,
      "[hybrid]": {
        "db": {
          "kind": "hana"
        }
      }
    }
  }
}
